{
    "name": "zhangdi/yii2-template-basic",
    "description": "Basic template for Yii2",
    "keywords": ["yii2", "framework", "project template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": ">=2.0.20",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "vlucas/phpdotenv": "v3.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        },
        "files": [
            "src/functions/functions.php"
        ]
    }
}
